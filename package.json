{
  "name": "football-manager-poc",
  "version": "1.0.0",
  "description": "Cross-platform football management simulation game - Proof of Concept",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export:web",
    "build:android": "eas build --platform android --profile production",
    "build:android-preview": "eas build --platform android --profile preview",
    "build:android-aab": "eas build --platform android --profile production-aab",
    "build:ios": "eas build --platform ios --profile production",
    "build:all": "eas build --platform all --profile production",
    "deploy:web": "expo export:web && echo 'Web build ready in web-build/ directory'",
    "test": "echo \"No tests specified yet\" && exit 0",
    "typecheck": "tsc --noEmit",
    "lint": "echo \"Linting not configured yet\" && exit 0"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.7.3",
    "@react-navigation/native": "^7.1.19",
    "expo": "~54.0.20",
    "expo-status-bar": "~3.0.8",
    "nanoid": "^5.1.6",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "typescript": "~5.9.2"
  },
  "keywords": [
    "football",
    "soccer",
    "manager",
    "simulation",
    "game",
    "react-native",
    "expo",
    "cross-platform"
  ],
  "author": "TwinCluster Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ParkNow914/twincluster.git"
  },
  "private": true
}
